\begin_layout Standard
Procedemos
\begin_inset Newline newline
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $f'\left(x\right)=\frac{\left(2x+1\right)\sqrt{x}-\left(x^{2}+x-1\right)\frac{1}{2\sqrt{x}}}{\left(\sqrt{x}\right)^{2}}=\frac{\left(2x+1\right)\sqrt{x}}{x}-\frac{\frac{x^{2}+x-1}{2\sqrt{x}}}{x}=\frac{\left(2x+1\right)x^{-\frac{1}{2}}}{1}-\frac{x^{2}+x-1}{2x^{\frac{1}{2}}x}=\frac{2x+1}{x^{\frac{1}{2}}}-\frac{x^{2}+x-1}{2x^{\frac{3}{2}}}$
\end_inset


\begin_inset Newline newline
\end_inset


\end_layout

\begin_layout Standard
multiplicamos por 2x 
\begin_inset Formula $\frac{2x+1}{x^{\frac{1}{2}}}$
\end_inset

para poder simplificar la resta de las fracciones.
\begin_inset Newline newline
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $f'(x)=\frac{4x^{2}+2x}{2x^{\frac{3}{2}}}-\frac{x^{2}+x-1}{2x^{\frac{3}{2}}}=\frac{4x^{2}+2x-x^{2}-x+1}{2\sqrt{x^{3}}}=$
\end_inset


